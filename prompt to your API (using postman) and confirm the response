import org.openqa.selenium.By;
import org.openqa.selenium.WebDriver;
import org.openqa.selenium.WebElement;
import org.openqa.selenium.chrome.ChromeDriver;
import org.openqa.selenium.support.ui.ExpectedConditions;
import org.openqa.selenium.support.ui.Select;
import org.openqa.selenium.support.ui.WebDriverWait;
import org.testng.annotations.AfterTest;
import org.testng.annotations.BeforeTest;
import org.testng.annotations.Test;
import java.time.Duration;
import java.util.List;

public class VehicleSearchTest {
    private WebDriver driver;
    private WebDriverWait wait;
    
    @BeforeTest
    public void setUp() {
        // Set Chrome driver path
        System.setProperty("webdriver.chrome.driver", "path/to/chromedriver");
        driver = new ChromeDriver();
        wait = new WebDriverWait(driver, Duration.ofSeconds(10));
        driver.manage().window().maximize();
        driver.get("https://example-vehicles.com");
    }
    
    @Test
    public void testVehicleSearchAndFilter() {
        // Select vehicle type
        Select vehicleType = new Select(driver.findElement(By.id("vehicle-type")));
        vehicleType.selectByVisibleText("SUV");
        
        // Select brand
        Select brand = new Select(driver.findElement(By.id("brand")));
        brand.selectByValue("toyota");
        
        // Set price range
        WebElement minPrice = driver.findElement(By.id("min-price"));
        minPrice.clear();
        minPrice.sendKeys("20000");
        
        WebElement maxPrice = driver.findElement(By.id("max-price"));
        maxPrice.clear();
        maxPrice.sendKeys("50000");
        
        // Set year range
        Select minYear = new Select(driver.findElement(By.id("min-year")));
        minYear.selectByVisibleText("2018");
        
        // Click search button
        WebElement searchButton = driver.findElement(By.id("search-button"));
        searchButton.click();
        
        // Wait for results and verify
        wait.until(ExpectedConditions.visibilityOfElementLocated(By.className("vehicle-results")));
        
        List<WebElement> results = driver.findElements(By.cssSelector(".vehicle-card"));
        assert !results.isEmpty() : "No search results found";
        
        // Verify filters are applied correctly
        for (WebElement result : results) {
            String priceText = result.findElement(By.className("price")).getText();
            int price = Integer.parseInt(priceText.replaceAll("[^0-9]", ""));
            assert price >= 20000 && price <= 50000 : "Price filter not working";
        }
    }
    
    @AfterTest
    public void tearDown() {
        if (driver != null) {
            driver.quit();
        }
    }
}
